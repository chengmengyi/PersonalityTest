<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.personalitytest.individuality">

    <application
        android:name="com.personalitytest.individuality.app.MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PersonalityTest">
        <activity
            android:name="com.personalitytest.individuality.page.LaunchActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.personalitytest.individuality.page.HomeActivity" />
        <activity android:name="com.personalitytest.individuality.page.question.QuestionActivity" />
        <activity android:name="com.personalitytest.individuality.page.answer_result.AnswerResultActivity" />
        <activity android:name="com.personalitytest.individuality.page.answer_result.OpenResultActivity" />
        <activity android:name="com.personalitytest.individuality.page.all_type.AllTypeActivity"
            android:launchMode="singleTask"/>
        <activity android:name="com.personalitytest.individuality.page.web.WebActivity" />

        <meta-data android:name="com.facebook.sdk.ApplicationId" android:value="@string/facebook_app_id"/>
    </application>

</manifest>